        }
    ],
    "outbounds": [
        {
            "alter_id": 2,
            "security": "none",
            "server": "120.240.167.132",
            "server_port": 30007,
            "tag": "proxy",
            "tls": {
                "enabled": true,
                "insecure": true,
                "server_name": "andromedae.weltknoten.xyz"
            },
            "type": "vmess",
            "uuid": "1e963efa-c099-3a7d-bf47-64db4ea46c8e"
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        },
        {
            "type": "dns",
            "tag": "dns_out"
        }
