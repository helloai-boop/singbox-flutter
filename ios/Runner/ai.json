{
    "log": {
        "level": "info",
        "output": "box.log"
    },
    "dns": {
        "servers": [
            {
                "tag": "dns_proxy",
                "address": "tcp://8.8.8.8",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "detour": "proxy"
            },
            {
                "tag": "dns_local",
                "address": "114.114.114.114",
